{
    "name": "scolib/bankcard",
    "type": "library",
    "description": "根据银行卡号识别所属银行以及卡类型",
    "keywords": [
        "scolib",
        "bankcard"
    ],
    "homepage": "https://github.com/scolib/bankcard",
    "license": "MIT",
    "authors": [
        {
            "name": "klgd",
            "email": "slice1213@gmail.com",
            "homepage": "http://www.scophp.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.1",
        "phpunit/phpunit": "~4.0||~5.0"
    },
    "autoload": {
        "psr-4": {
            "Sco\\Bankcard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sco\\Bankcard\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fix-style": "php-cs-fixer fix ./"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
